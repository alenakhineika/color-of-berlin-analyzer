{
  "name": "color-of-berlin-palette",
  "displayName": "color-of-berlin-palette",
  "version": "0.0.1",
  "description": "Fetch colors of the sky in Berlin and build a color palette.",
  "author": "Alena Khineika <alena.khineika@gmail.com>",
  "license": "ISC",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "save-tweets-to-file": "npm run build && node build/scripts/save-tweets-to-file.js",
    "save-tweets-to-mongodb": "npm run build && node build/scripts/save-tweets-to-mongodb.js",
    "start": "npm run build && node build/index.js",
    "lint": "eslint '**/*.{js,ts}'",
    "check": "npm run lint",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "precommit": [
    "check"
  ],
  "dependencies": {
    "dotenv": "8.2.0",
    "fs-extra": "^9.1.0",
    "mongodb": "^3.6.3",
    "twitter": "1.7.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "bson": "^4.2.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.5",
    "typescript": "^4.1.3"
  }
}
