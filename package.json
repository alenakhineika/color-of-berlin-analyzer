{
  "name": "color-of-berlin-palette",
  "displayName": "color-of-berlin-palette",
  "version": "0.0.1",
  "description": "Fetch colors of the sky in Berlin and create a color palette.",
  "author": "Alena Khineika <alena.khineika@gmail.com>",
  "license": "ISC",
  "scripts": {
    "save-tweets-to-file": "npm run build:server && node build/scripts/save-tweets-to-file.js",
    "save-tweets-to-mongodb": "npm run build:server && node build/scripts/save-tweets-to-mongodb.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "tsc",
    "start": "npm run build:client && npm run start:server",
    "start:client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "start:server": "npm run build:server && node build/",
    "start:dev": "concurrently \"nodemon\" \"npm run start:client\"",
    "start:dev-server": "nodemon",
    "lint": "eslint '**/*.{js,ts}'",
    "check": "npm run lint",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "precommit": [
    "check"
  ],
  "dependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.2.2",
    "dotenv": "8.2.0",
    "fs-extra": "^9.1.0",
    "mongodb": "^3.6.3",
    "twitter": "1.7.1",
    "bson": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.6",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.0.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.17.3",
    "ora": "^5.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.5",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "typescript": "^4.1.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
